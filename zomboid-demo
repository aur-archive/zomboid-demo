#!/bin/sh
java -Djava.library.path=/usr/lib:/usr/share/lwjgl/native/linux/ -Dsun.java2d.noddraw=true -Dsun.awt.noerasebackground=true -Dsun.java2d.d3d=false -Dsun.java2d.opengl=false -Dsun.java2d.pmoffscreen=false \
-cp /usr/share/java/zomboid-demo/zombie.jar:/usr/share/lwjgl/jar/lwjgl.jar:/usr/share/lwjgl/jar/lwjgl_util.jar:/usr/share/lwjgl/jar/jinput.jar zombie.FrameLoader
